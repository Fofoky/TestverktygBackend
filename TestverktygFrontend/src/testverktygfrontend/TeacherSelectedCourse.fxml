<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testverktygfrontend.TeacherSelectedCourseController">
   <children>
      <ImageView fitHeight="502.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pictures/Kursida.png" />
         </image>
      </ImageView>
      <Button layoutX="19.0" layoutY="376.0" mnemonicParsing="false" onAction="#deleteTestButton" prefHeight="25.0" prefWidth="80.0" text="Radera test" />
      <Button layoutX="179.0" layoutY="376.0" mnemonicParsing="false" onAction="#updateTestButton" prefHeight="25.0" prefWidth="80.0" text="Ändra test" />
      <Button layoutX="99.0" layoutY="376.0" mnemonicParsing="false" onAction="#createTestButton" prefHeight="25.0" prefWidth="80.0" text="Nytt test" />
      <Label fx:id="labelSelectedTest" layoutX="279.0" layoutY="91.0" text="Namn på valt test" />
      <Label fx:id="labelCourse" layoutX="20.0" layoutY="90.0" text="Label: namn på kurs" />
      <TableView fx:id="tableTests" editable="true" layoutX="19.0" layoutY="107.0" prefHeight="268.0" prefWidth="239.0">
         <columns>
            <TableColumn fx:id="columnTest" editable="false" prefWidth="71.0" text="Prov" />
            <TableColumn fx:id="columnStart" onEditCommit="#handleEditStartTime" prefWidth="81.0" text="Start" />
            <TableColumn fx:id="columnStop" minWidth="6.0" onEditCommit="#handleEditStopTime" prefWidth="86.0" text="Finish" />
         </columns>
      </TableView>
      <TableView fx:id="tableStudentTestResult" layoutX="278.0" layoutY="107.0" prefHeight="293.0" prefWidth="203.0">
         <columns>
            <TableColumn fx:id="columnStudent" prefWidth="110.0" text="Elev" />
            <TableColumn fx:id="columnResult" prefWidth="92.0" text="Resultat" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
