<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testverktygfrontend.TeacherSelectedCourseController">
   <children>
      <ImageView fitHeight="519.0" fitWidth="510.0" layoutX="-10.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pictures/Kursida.png" />
         </image>
      </ImageView>
      <Button layoutX="383.0" layoutY="160.0" mnemonicParsing="false" onAction="#deleteTestButton" prefWidth="90.0" text="Radera test" />
      <Button layoutX="383.0" layoutY="193.0" mnemonicParsing="false" onAction="#updateTestButton" prefWidth="90.0" text="Ändra test" />
