<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testverktygfrontend.TeacherSelectedCourseController">
   <children>
      <BorderPane focusTraversable="true" prefHeight="500.0" prefWidth="500.0">
         <center>
            <Pane prefHeight="427.0" prefWidth="298.0">
               <children>
                  <TableView fx:id="tableTests" layoutX="-1.0" layoutY="29.0" prefHeight="150.0" prefWidth="328.0" BorderPane.alignment="CENTER">
                     <columns>
                        <TableColumn fx:id="columnTest" prefWidth="89.0" text="Prov" />
                        <TableColumn fx:id="columnStart" prefWidth="95.0" text="startTime" />
                        <TableColumn fx:id="columnStop" prefWidth="111.0" text="endTime" />
                        <TableColumn fx:id="columnStatus" prefWidth="107.0" text="Status" />
                     </columns>
                  </TableView>
                  <TableView fx:id="tableStudentTestResult" layoutX="-1.0" layoutY="262.0" prefHeight="150.0" prefWidth="445.0">
                     <columns>
                        <TableColumn fx:id="columnStudent" prefWidth="262.0" text="Elev" />
                        <TableColumn fx:id="columnResult" prefWidth="169.0" text="Resultat" />
                     </columns>
                  </TableView>
                  <Label fx:id="labelSelectedTest" layoutX="-1.0" layoutY="232.0" text="Namn på valt test" />
               </children>
            </Pane>
         </center>
         <padding>
            <Insets bottom="20.0" left="30.0" right="30.0" top="20.0" />
         </padding>
         <top>
            <VBox layoutX="54.0" layoutY="69.0">
               <children>
                  <Text layoutX="54.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Kurssida " />
                  <Label fx:id="labelCourse" layoutX="54.0" layoutY="92.0" text="Label: namn på kurs" />
               </children>
            </VBox>
         </top>
         <right>
            <Pane>
               <children>
                  <VBox alignment="TOP_RIGHT" layoutY="89.0" prefHeight="142.0" prefWidth="111.0" spacing="4.0">
                     <children>
                        <Button fx:id="buttonEditTest" mnemonicParsing="false" prefWidth="90.0" text="Ändra test" />
                        <Button fx:id="buttonDeleteTest" mnemonicParsing="false" onAction="#deleteTestButton" prefWidth="90.0" text="Radera test" />
                        <Button fx:id="buttonCreateTest" layoutX="7.0" layoutY="133.0" mnemonicParsing="false" onAction="#createTestButton" prefWidth="90.0" text="Nytt test" />
                     </children>
                  </VBox>
               </children>
            </Pane>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
