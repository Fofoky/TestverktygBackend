      <Label fx:id="labelCourse" layoutX="24.0" layoutY="269.0" text="Label: namn på kurs" />
      <TableView fx:id="tableTests" editable="true" layoutX="47.0" layoutY="91.0" prefHeight="150.0" prefWidth="328.0">
         <columns>
            <TableColumn fx:id="columnTest" editable="false" prefWidth="62.0" text="Prov" />
